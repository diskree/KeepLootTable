{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modVersion}",
  "name": "${modName}",
  "description": "Loot table will be stored in the NBT after loot is generated.",
  "authors": [
    "${author}"
  ],
  "contact": {
    "sources": "${repoUrl}"
  },
  "license": "MIT",
  "icon": "assets/keeploottable/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.diskree.keeploottable.KeepLootTable"
    ]
  },
  "mixins": [
    "keeploottable.mixins.json"
  ],
  "depends": {
    "java": ">=${minJavaVersion}",
    "minecraft": ">=${minecraftVersion}",
    "fabricloader": ">=${minLoaderVersion}"
  },
  "accessWidener": "keeploottable.accesswidener"
}
